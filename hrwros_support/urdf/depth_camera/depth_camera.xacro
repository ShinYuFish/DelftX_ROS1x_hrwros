<?xml version="1.0"?>
<robot name="depth_camera" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:arg name="prefix" default="depth_camera_"/>

  <!-- URDF -->
  <xacro:include filename="$(find hrwros_support)/urdf/depth_camera/depth_camera.urdf.xacro"/>
  <xacro:depth_camera_urdf prefix="$(arg prefix)"/>

  <!-- Gazebo -->
  <xacro:include filename="$(find hrwros_support)/urdf/depth_camera/depth_camera.gazebo.xacro"/>
  <xacro:depth_camera_gazebo prefix="$(arg prefix)"/>

</robot>
